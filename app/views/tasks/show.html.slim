p id="notice" = notice

.well
  p
    strong Name:
    = @task.name

  p
    strong Story Point:
    = @task.story_point

  p
    strong End Date:
    = @task.expected_end_date

  p
    strong Progress:
    = @task.progress_status

= link_to 'Edit', edit_task_path(@task), class: "btn btn-success"
span style="padding-right: 20px"
= link_to 'Back', tasks_path, class: "btn btn-default"
